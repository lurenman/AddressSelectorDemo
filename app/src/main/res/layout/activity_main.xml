<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context="com.example.administrator.addressselectordemo.MainActivity">

    <TextView
        android:id="@+id/tv_New_Activity_SelectCities"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="（原）信使公司的地址选择展示"
        android:textSize="18sp"
        android:textColor="@color/colorPrimary"
        android:padding="10dp"
        android:layout_marginTop="60dp"
        />
    <TextView
        android:id="@+id/tv_AddressSelector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="仿京东地址选择器"
        android:textSize="18sp"
        android:textColor="@color/colorPrimary"
        android:padding="10dp"
        android:layout_marginTop="60dp"
        />

    <TextView
        android:id="@+id/tv_AddressSelector1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="另一个仿京东地址选择器"
        android:textSize="18sp"
        android:textColor="@color/colorPrimary"
        android:padding="10dp"
        android:layout_marginTop="60dp"
        />
</LinearLayout>
